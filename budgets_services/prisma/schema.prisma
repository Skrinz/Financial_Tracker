generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Budget {
  id        Int      @id @default(autoincrement())
  userId    Int
  budget_name  String
  amount    Float
  createdAt DateTime @default(now())
  expenses  Int
}